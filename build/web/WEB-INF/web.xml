<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ReadFile</servlet-name>
        <servlet-class>br.com.servlet.ReadFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Operations</servlet-name>
        <servlet-class>br.com.servlet.Operations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Relations</servlet-name>
        <servlet-class>br.com.servlet.Relations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Composition</servlet-name>
        <servlet-class>br.com.servlet.Composition</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReadFile</servlet-name>
        <url-pattern>/ReadFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Operations</servlet-name>
        <url-pattern>/Operations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Relations</servlet-name>
        <url-pattern>/Relations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Composition</servlet-name>
        <url-pattern>/Composition</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
